generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}
model Notice{
  id Int @id @default(autoincrement())
  noticeTitle   String
  noticeDate    String
}
model WriteForm{
  id Int @id @default(autoincrement())
  firstName String
  lastName String
  birthday String
  school String?
  ageProof String
  teamMember String?
  section String
  ageCategory String
  email String
  phone String
  videoLink String
  depostisor String
  teacher String
  teacherEmail String
  performingPiece Json
}